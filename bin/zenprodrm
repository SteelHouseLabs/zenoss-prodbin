. $ZENHOME/bin/zenfunctions

cd $ZENHOME/Products

for d in $ZENPRODUCTS
do
    echo Removing product $d...
    rm -rf $d
    rm -f VERSION.txt
done
rm -rf .svn
rm -f *.txt
exit;
