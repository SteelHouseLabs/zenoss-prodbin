<script>
function changeDetail() {
    var where = window.location.href.split('#')[1],
        parts = where.split(':'),
        contextId = parts[1].replace(/\./g, '/'),
        panelKey = parts[2],
        backCompatPanel = Ext.getCmp(panelKey);
    backCompatPanel.setContext(contextId);
}
</script>
