<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    xmlns:five="http://namespaces.zope.org/five">

<browser:viewlet
        name="extjs-base"
        weight="0"
        manager=".interfaces.IJavaScriptSrcManager"
        class=".javascript.ExtBaseJs"
        permission="zope2.Public"
        />

<browser:viewlet
        name="firefox-compat"
        weight="1"
        manager=".interfaces.IJavaScriptSrcManager"
        class=".javascript.FireFoxExtCompat"
        permission="zope2.Public"
        />

<browser:viewlet
        name="extjs-all"
        weight="2"
        manager=".interfaces.IJavaScriptSrcManager"
        class=".javascript.ExtAllJs"
        permission="zope2.Public"
        />

<browser:viewlet
        name="livegrid-all"
        weight="3"
        manager=".interfaces.IJavaScriptSrcManager"
        class=".javascript.LiveGridJs"
        permission="zope2.Public"
        />
        
<browser:viewlet
        name="js-all"
        paths="/++resource++extjs/treegrid/TreeGridSorter.js
               /++resource++extjs/treegrid/TreeGridColumnResizer.js
               /++resource++extjs/treegrid/TreeGridNodeUI.js
               /++resource++extjs/treegrid/TreeGridLoader.js
               /++resource++extjs/treegrid/TreeGridColumns.js
               /++resource++extjs/fileuploadfield/FileUploadField.js               
               /++resource++extjs/treegrid/TreeGridLoader.js
               /++resource++extjs/treegrid/TreeGridColumns.js
               /++resource++extjs/treegrid/TreeGrid.js
               extdirect.js"
        weight="4"
        manager=".interfaces.IJavaScriptSrcManager"
        class=".javascript.JavaScriptSrcBundleViewlet"
        permission="zope2.Public"
        />

<browser:viewlet
        name="zenoss-all"
        weight="5"
        manager=".interfaces.IJavaScriptSrcManager"
        class=".javascript.ZenossAllJs"
        permission="zope2.Public"
        />

<browser:viewlet
        name="i18njs"
        paths="i18n.js"
        weight="6"
        manager=".interfaces.IJavaScriptSrcManager"
        class=".javascript.JavaScriptSrcBundleViewlet"
        permission="zope2.Public"
        />

</configure>
