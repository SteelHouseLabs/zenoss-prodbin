<configure xmlns="http://namespaces.zope.org/zope">
    <include package="Products.Five" file="meta.zcml"/>

    <!-- publishing model changes -->
  <subscriber
        for="Products.ZenUtils.guid.interfaces.IGloballyIdentifiable
             zope.app.container.interfaces.IObjectAddedEvent"
        handler=".subscribers.publishAdd"
        />

    <subscriber
        for="Products.ZenUtils.guid.interfaces.IGloballyIdentifiable
             zope.app.container.interfaces.IObjectRemovedEvent"
        handler=".subscribers.publishRemove"
        />

    <subscriber
        for="Products.ZenModel.DeviceOrganizer.DeviceOrganizer
             zope.app.container.interfaces.IObjectAddedEvent"
        handler=".subscribers.publishAdd"
        />

    <subscriber
        for="Products.ZenModel.DeviceOrganizer.DeviceOrganizer
             zope.app.container.interfaces.IObjectRemovedEvent"
        handler=".subscribers.publishRemove"
        />

    <subscriber
        for="Products.ZenUtils.guid.interfaces.IGloballyIdentifiable
             Products.ZenModel.ChangeEvents.interfaces.IObjectModifiedEvent"
        handler=".subscribers.publishModified"
        />

    <subscriber
        for="Products.ZenUtils.guid.interfaces.IGloballyIdentifiable
             Products.ZenModel.ChangeEvents.interfaces.IObjectAddedToOrganizerEvent"
        handler=".subscribers.publishAddEdge"
        />

    <subscriber
        for="Products.ZenUtils.guid.interfaces.IGloballyIdentifiable
             Products.ZenModel.ChangeEvents.interfaces.IObjectRemovedFromOrganizerEvent"
        handler=".subscribers.publishRemoveEdge"
        />

    <subscriber
        for="Products.ZenModel.Device.Device
             Products.ZenModel.ChangeEvents.interfaces.IDeviceClassMoveEvent"
        handler=".subscribers.publishObjectMove"
        />

</configure>