<script type="text/javascript" src="/zport/js/FormCollect.js"></script>
<script language="javascript">
function getImage(form)
{
    var img = document.getElementById(form.name.value + ":image");
    var now = new Date();
    img.src = form.action + "?" + FormCollect(form) + '&u=' + now.getTime();
}

function toggleForm(image)
{
   if (!image) return;
   var form = eval('document.' + image.id.split(":")[0]);
   if (form.visible == 0) {
      showElement(form);
      form.visible = 1;
   } else {
      hideElement(form);
      form.visible = 0;
   }
}

function hideForms() {
    for (i = 0; i < document.forms.length; i++) {
       var form = document.forms[i];
       if (form.name && document.getElementById(form.name.value + ':image')) {
          hideElement(form);
          form.visible = 0;
       }
    }
}
//-->
</script>

<h3>Click the graph to edit</h3>

<form name="aggregateDisks" action="/zport/RenderServer/plugin" 
      onsubmit="getImage(this); return false">
    <input type="hidden" name="name" value="aggregateDisks"/>
   <table class="zentable" cellpadding="3" cellspacing="1">
     <tr><th class="tabletitle" colspan="4">
           Edit graph parameters below, click the image to hide this form.
         </th>
     </tr><tr>
        <td class="tablevalues">Width:</td>
        <td class="tablevalues"><input class="tablevalues" name="width" value="500"/></td>
        <td class="tablevalues">Height:</td>
        <td class="tablevalues"><input class="tablevalues" name="height" value="100"/></td>
     </tr><tr>
        <td class="tablevalues">Devices:</td>
        <td class="tablevalues"><input class="tablevalues" name="devices" value=".*"/></td>
        <td class="tablevalues">Span:</td>
        <td class="tablevalues">
            <select class="tablevalues" 
                    name="start" onchange="getImage(this.form)">
                <option value="-1d">One Day</option>
                <option value="-1w" selected="True">One Week</option>
                <option value="-2w">Two Weeks</option>
                <option value="-1m">One Month</option>
                <option value="-1y">One Year</option>
            </select>
        </td>
     </tr><tr>
        <td class="tablevalues">
           <input type='checkbox' 
                  class="tablevalues" 
                  onchange="getImage(this.form)" 
                  name='nolimit'/>Hide Limit
        </td>
        <td colspan="3" class="tablevalues"/>
     </tr><tr>
        <td class="tablevalues" colspan="4"><input class="tablevalues" type="submit"></td>
     </tr>
    </table>
</form>
<img id="aggregateDisks:image" onclick="toggleForm(this)" 
     alt="Onos! Where is the graph?"/>


