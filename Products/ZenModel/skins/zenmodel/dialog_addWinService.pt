<h2>Add WinService</h2>
<input type="hidden" name="userCreated" value="True">
<br/>
<div id="winServiceClassLiveSearch" style="width:30em;position:relative;">
</div>
<br/><br/>
<div id="dialog_buttons">
<input tal:attributes="id string:dialog_submit;
        disabled string:true;
        type string:submit;
        value string:OK;
        onclick string:return $$('dialog').submit_form('${here/os/absolute_url}')"
        name="addWinService:method" />
<input tal:attributes="id string:dialog_cancel;
                        type string:button;
                        value string:Cancel;
                        onclick string:$$('dialog').hide()" />
</div>
