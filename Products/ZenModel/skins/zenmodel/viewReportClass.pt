<tal:block metal:use-macro="here/templates/macros/page3">
<tal:block metal:fill-slot="contentPane">

<form method="post" tal:attributes="action here/absolute_url_path">
<input type="hidden" name="zenScreenName" tal:attributes="value template/id"/>


        <tal:block tal:define="tableName string:reportorgs;
        menu_id string:ReportClass_list;
        objects here/children;
        showfilterbox python:True;
        batch python:here.ZenTableManager.getBatch(tableName,objects); 
        tabletitle string:Report Organizers"
        tal:condition="objects">
        <tal:block metal:use-macro="here/zenuimacros/macros/zentable">

        <!--====Optional titlebar slots=============

            <tal:block metal:fill-slot="filterslot">
            </tal:block>

            <tal:block metal:fill-slot="menuslot">
            </tal:block>

            ==========================================-->

        <tal:block metal:fill-slot="zentablecontents">
        <!-- BEGIN TABLE CONTENTS -->
            
    
    <tr tal:condition="objects">
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'primarySortKey','Name')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'countChildren','SubFolders')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'countReports','Reports')"/>
    </tr>
    <tal:block tal:repeat="rorg batch"> 
    <tr tal:define="odd repeat/rorg/odd"
        tal:attributes="class python:test(odd, 'odd', 'even')">
      <td class="tablevalues">
        <input tal:condition="here/isManager" 
            type="checkbox" name="organizerPaths:list" 
            tal:attributes="value rorg/id"/>
      <a tal:attributes="href rorg/getPrimaryUrlPath" tal:content="rorg/id"></a>
      </td>
      <td class="tablevalues" tal:content="rorg/countChildren"/>
      <td class="tablevalues" tal:content="rorg/countReports"/>
    </tr>
    </tal:block>
    <tr>
        <td class="tableheader" colspan="3">
            <span metal:use-macro="here/zenTableNavigation/macros/navbody"/>
        </td>
    </tr>
    <tr tal:condition="here/isManager">
        <td class="tableheader" colspan="3">
            <span metal:use-macro="here/deviceOrganizerStatus/macros/editor"/>
        </td>
    </tr>

        <!-- END TABLE CONTENTS -->

        </tal:block>
        </tal:block>
        </tal:block>
        

</form>
<br>
<form method="post" tal:attributes="action here/absolute_url_path">
<input type="hidden" name="zenScreenName" tal:attributes="value template/id"/>


        <tal:block tal:define="tableName string:reportlist;
        objects here/reports;
        showfilterbox python:True;
        batch python:here.ZenTableManager.getBatch(tableName,objects); 
        tabletitle string:Reports"
        tal:condition="objects">
        <tal:block metal:use-macro="here/zenuimacros/macros/zentable">

        <!--====Optional titlebar slots=============

            <tal:block metal:fill-slot="filterslot">
            </tal:block>

            <tal:block metal:fill-slot="menuslot">
            </tal:block>

            ==========================================-->

        <tal:block metal:fill-slot="zentablecontents">
        <!-- BEGIN TABLE CONTENTS -->
            
    
    <tr tal:condition="objects">
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'primarySortKey','Name')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'description','Description')"/>
    </tr>
    <tal:block tal:repeat="rpt batch" tal:omit-tag="">
    <tr tal:define="odd repeat/rpt/odd"
        tal:attributes="class python:test(odd, 'odd', 'even')">
      <td class="tablevalues">
        <input tal:condition="here/isManager" type="checkbox" name="ids:list" 
            tal:attributes="value rpt/id"/>
      <a tal:attributes="href rpt/getPrimaryUrlPath" tal:content="rpt/id"></a>
      </td>
      <td class="tablevalues" tal:content="rpt/description"/>
    </tr>
    </tal:block>
    <tr>
        <td colspan="2" class="tableheader">
            <span metal:use-macro="here/zenTableNavigation/macros/navbody"/>
        </td>
    </tr>
    <tr tal:condition="python:here.isManager() and here.id=='Device Reports'">
    <td class="tableheader" colspan="2">
        <input class="tableheader" type="text" name="id"/>
        <input class="tableheader" type="submit" 
            name="manage_addDeviceReport:method" value=" Add " /> 
        |
        <input class="tableheader" type="submit" name="moveReports:method"
                value="Move To" />
        <select class="tableheader" name="moveTarget"
        tal:define="targets here/childMoveTargets">
            <option value=""></option>
            <option tal:repeat="target targets" 
                tal:content="target">/Server/Linux</option>
        </select> 
        |
        <input class="tableheader" type="submit" 
            name="zmanage_delObjects:method" value=" Delete " />
        |
        <input class="tableheader" type="submit"
                name="SelectButton" value="Select All" 
                onClick="toggleSelect(this.form);return false"/>
    </td>
    </tr>

        <!-- END TABLE CONTENTS -->

        </tal:block>
        </tal:block>
        </tal:block>
        

</form>
</tal:block>
</tal:block>
