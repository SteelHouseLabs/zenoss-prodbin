<tal:block metal:use-macro="here/templates/macros/page2">
<tal:block metal:fill-slot="contentPane"
    tal:define="editable python:here.isEditable(here)">

<form method="POST" tal:attributes="action here/absolute_url_path">
<input type="hidden" name="zenScreenName" tal:attributes="value template/id"/>


        <tal:block tal:define="message request/message | string:State at time:; 
            message request/message | string:State at time:;
            tabletitle string:${message} ${here/getNowString}">
        <tal:block metal:use-macro="here/zenuimacros/macros/zentable">

        <!--====Optional titlebar slots=============

            <tal:block metal:fill-slot="filterslot">
            </tal:block>

            <tal:block metal:fill-slot="menuslot">
            </tal:block>

            ==========================================-->

        <tal:block metal:fill-slot="zentablecontents">
        <!-- BEGIN TABLE CONTENTS -->
            
    <tr>
        <td class="tableheader">Name</td>
        <td class="tablevalues" tal:condition="here/isManager">
            <input class="tablevalues" type="text" name="newId" size="40"
                tal:attributes="value here/id" />
        </td>
        <td class="tablevalues" tal:condition="not:here/isManager"
            tal:content="here/id"/>
    </tr>
    <tr>
    <td class="tableheader" colspan="2">Description</td>
    </tr>
    <tr>
    <td class="tablevalues" colspan="2">
        <textarea class="tablevalues" rows="2" cols="60" name="description:text"
            tal:content="here/description">
        </textarea>
    </td>
    </tr>
    <tr>
        <td class="tableheader" colspan="2">
            <input class="tableheader" type="submit" value="Save"
                name="zmanage_editProperties:method" />
        </td>
    </tr>

        <!-- END TABLE CONTENTS -->

        </tal:block>
        </tal:block>
        </tal:block>
        

</form>
<br/>

<form tal:attributes="method string:post;
                action here/absolute_url_path;
                name string:datasourceList">
<input type="hidden" name="zenScreenName" tal:attributes="value template/id"/>


        <tal:block tal:define="tableName string:rrdtempdslist; 
        objects here/getRRDDataSources;
        batch python:here.ZenTableManager.getBatch(tableName,objects); 
        tabletitle string:Data Sources;
		menu_id string:DataSource_list;">
        <tal:block metal:use-macro="here/zenuimacros/macros/zentable">

        <!--====Optional titlebar slots=============

            <tal:block metal:fill-slot="filterslot">
            </tal:block>

            <tal:block metal:fill-slot="menuslot">
            </tal:block>

            ==========================================-->

        <tal:block metal:fill-slot="zentablecontents">
        <!-- BEGIN TABLE CONTENTS -->
            
    <input type='hidden' name='tableName' tal:attributes="value tableName" />
    <tal:block metal:define-macro="dsbody">
    <tr>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'getId','Name')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'oidorurl','Source')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'sourcetype','Source Type')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'enabled','Enabled')"/>
    </tr>
    <tr class="tablevalues" tal:repeat="ds batch">
        <td>
            <input tal:condition="editable" 
                type="checkbox" name="ids:list" 
                tal:attributes="value ds/getId"/>
            <a tal:condition="editable"
                tal:attributes="href ds/getPrimaryUrlPath" 
                tal:content="ds/getId">name</a>
            <tal:block tal:condition="not:editable" tal:content="ds/getId"/>
        </td>
        <td tal:content="ds/getDescription"/>
        <td tal:content="ds/sourcetype"/>
        <td tal:content="ds/enabled"/>
    </tr>
    <tr>
        <td colspan="4" class="tableheader">
            <span metal:use-macro="here/zenTableNavigation/macros/navbody"/>
        </td>
    </tr>
    </tal:block>

        <!-- END TABLE CONTENTS -->

        </tal:block>
        </tal:block>
        </tal:block>
        

</form>
<br/>
<form tal:attributes="method string:post;
                action here/absolute_url_path;
                name string:thresholdList">
<input type="hidden" name="zenScreenName" tal:attributes="value template/id"/>


        <tal:block tal:define="tableName string:rrdthreshsoldist; 
        objects here/thresholds;
        batch python:here.ZenTableManager.getBatch(tableName,objects); 
        tabletitle string:Thresholds;
		menu_id string:Threshold_list">
        <tal:block metal:use-macro="here/zenuimacros/macros/zentable">

        <!--====Optional titlebar slots=============

            <tal:block metal:fill-slot="filterslot">
            </tal:block>

            <tal:block metal:fill-slot="menuslot">
            </tal:block>

            ==========================================-->

        <tal:block metal:fill-slot="zentablecontents">
        <!-- BEGIN TABLE CONTENTS -->
            
    <input type='hidden' name='tableName' tal:attributes="value tableName" />
    <tal:block metal:define-macro="threshbody">
    <tr>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'getId','Name')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'dsnames','Data Sources')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'severity','Severity')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'escalateCount','Escalate Count')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'enabled','Enabled')"/>
    </tr>
    <tr class="tablevalues" tal:repeat="thresh batch">
        <td>
            <input tal:condition="editable" 
                type="checkbox" name="ids:list" 
                tal:attributes="value thresh/getId"/>
            <a tal:condition="editable"
                tal:attributes="href thresh/getPrimaryUrlPath" 
                tal:content="thresh/getId">name</a>
            <tal:block tal:condition="not:editable" tal:content="thresh/getId"/>
        </td>
        <td tal:content="python:', '.join(thresh.dsnames)"/>
        <td tal:content="thresh/getSeverityString"/>
        <td tal:content="thresh/escalateCount"/>
        <td tal:content="thresh/enabled"/>
    </tr>
    <tr>
        <td colspan="5" class="tableheader">
            <span metal:use-macro="here/zenTableNavigation/macros/navbody"/>
        </td>
    </tr>
    </tal:block>

        <!-- END TABLE CONTENTS -->

        </tal:block>
        </tal:block>
        </tal:block>
        

</form>
<br/>
<form tal:attributes="method string:post;
                action here/absolute_url_path;
                name string:graphList">
<input type="hidden" name="zenScreenName" tal:attributes="value template/id"/>


        <tal:block tal:define="tableName string:rrdgraphlist; 
        objects here/getGraphs;
        batch python:here.ZenTableManager.getBatch(tableName,objects,
                                                sortedHeader='sequence'); 
        tabletitle string:Graphs;
		menu_id string:Graph_list">
        <tal:block metal:use-macro="here/zenuimacros/macros/zentable">

        <!--====Optional titlebar slots=============

            <tal:block metal:fill-slot="filterslot">
            </tal:block>

            <tal:block metal:fill-slot="menuslot">
            </tal:block>

            ==========================================-->

        <tal:block metal:fill-slot="zentablecontents">
        <!-- BEGIN TABLE CONTENTS -->
            
    <input type='hidden' name='tableName' tal:attributes="value tableName" />
    <tal:block metal:define-macro="graphbody">
    <tr>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'sequence','Seq')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'getId','Name')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'dsnames','Data Sources')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'units','Units')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'height','Height')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'width','Width')"/>
    </tr>
    <tr class="tablevalues" tal:repeat="graph batch">
        <td>
            <input tal:condition="editable" 
                type="text" name="seqmap:list" size="2"
                tal:attributes="value graph/sequence"/>
            <input tal:condition="editable"
                type="hidden" name="origseq:list"
                tal:attributes="value graph/sequence" />
            <span tal:condition="not:editable" tal:content="graph/sequence"/>
        </td>
        <td>
            <input tal:condition="editable" 
                type="checkbox" name="ids:list" 
                tal:attributes="value graph/getId"/>
            <a tal:condition="editable"
                tal:attributes="href graph/getPrimaryUrlPath" 
                tal:content="graph/getId">name</a>
            <tal:block tal:condition="not:editable" tal:content="graph/getId"/>
        </td>
        <td>
            <tal:block tal:repeat="ds graph/dsnames">
                <tal:block tal:replace="ds" /><tal:block tal:condition="python: ds not in here.getRRDDataPointNames()"><span class='errorvalues'> (invalid)</span></tal:block><tal:block tal:condition="not: repeat/ds/end">, </tal:block>
            </tal:block>
        </td>
        <td tal:content="graph/units"/>
        <td tal:content="graph/height"/>
        <td tal:content="graph/width"/>
    </tr>
    <tr>
        <td colspan="6" class="tableheader">
            <span metal:use-macro="here/zenTableNavigation/macros/navbody"/>
        </td>
    </tr>
    </tal:block>

        <!-- END TABLE CONTENTS -->

        </tal:block>
        </tal:block>
        </tal:block>
        
</form>

</tal:block>
</tal:block>
