<tal:block metal:use-macro="here/templates/macros/page2">
<tal:block metal:fill-slot="contentPane"
    tal:define="editable here/isManager">
<form method="post" tal:attributes="action here/absolute_url_path">
<input type="hidden" name="zenScreenName" tal:attributes="value template/id"/>
<table class="zentable"  cellpadding="4" cellspacing="1">
    <tr tal:define="message request/message | string:State at time:">
        <th class=tabletitle colspan="2"
            tal:content="string:${message} ${here/getNowString}" />
    </tr>
    <tr>
        <td class="tableheader">Name</td>
        <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" type="text" name="newId" size="40"
                tal:attributes="value here/id" />
        </td>
        <td class="tablevalues" tal:condition="not:editable"
            tal:content="here/id"/>
    </tr>
    <tr>
    <td class="tableheader">Language</td>
        <td class="tablevalues" tal:condition="editable">
            <input class="tablevalues" type="text" name="language" size="40"
                tal:attributes="value here/language" />
        </td>
        <td class="tablevalues" tal:condition="not:editable"
            tal:content="here/id"/>
    </tr>
    <tr>
    <td class="tableheader" colspan="2">Contact</td>
    </tr>
    <tr>
    <td class="tablevalues" colspan="2">
        <textarea class="tablevalues" rows="2" cols="80" name="contact:text"
            tal:content="here/contact">
        </textarea>
    </td>
    </tr>
    <tr>
    <td class="tableheader" colspan="2">Description</td>
    </tr>
    <tr>
    <td class="tablevalues" colspan="2">
        <textarea class="tablevalues" rows="5" cols="80" name="description:text"
            tal:content="here/description">
        </textarea>
    </td>
    </tr>
    <tr>
        <td class="tableheader" colspan="2">
            <input class="tableheader" type="submit" value="Save"
                name="zmanage_editProperties:method" />
        </td>
    </tr>
</table>
</form>
<br/>
<form method="post" tal:attributes="action here/absolute_url_path">
<input type="hidden" name="zenScreenName" tal:attributes="value template/id"/>
<table class="zentable" cellpadding="3" cellspacing="1"
    tal:define="tableName string:mibmodulenodeslist; 
        objects here/nodes;
        batch python:here.ZenTableManager.getBatch(tableName,objects)">
    <input type='hidden' name='tableName' tal:attributes="value tableName" />
    <tr>
      <th class="tabletitle" align="left" colspan="3">RRDDataSources</th>
    </tr>
    <tr>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'getId','Name')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'oid','OID')"/>
        <th tal:replace="structure python:here.ZenTableManager.getTableHeader(
                    tableName,'nodetype','Type')"/>
    </tr>
    <tr class="tablevalues" tal:repeat="ds batch">
        <td>
            <a tal:condition="editable"
                tal:attributes="href ds/getPrimaryUrlPath" 
                tal:content="ds/getId">name</a>
            <tal:block tal:condition="not:editable" tal:content="ds/getId"/>
        </td>
        <td tal:content="ds/oid"/>
        <td tal:content="ds/nodetype"/>
    </tr>
    <tr>
        <td colspan="3" class="tableheader">
            <span metal:use-macro="here/zenTableNavigation/macros/navbody"/>
        </td>
    </tr>
</table>
</form>
<br/>
<form method="post" tal:attributes="action here/absolute_url_path">
<input type="hidden" name="zenScreenName" tal:attributes="value template/id"/>
<table class="zentable" cellpadding="3" cellspacing="1"
    tal:define="tableName string:mibmodulenodeslist; 
