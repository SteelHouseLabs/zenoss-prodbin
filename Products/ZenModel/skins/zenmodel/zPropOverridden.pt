<tal:block metal:use-macro="here/templates/macros/page2">
<tal:block metal:fill-slot="contentPane">
<tal:block tal:define="tabletitle string:Overriding Objects;
	               prop request/prop | python:'';
	               orObjects python:here.getOverriddenObjects(prop)"> 
<tal:block metal:use-macro="here/zenuimacros/macros/zentable">
<tal:block metal:fill-slot="zentablecontents">

<!-- BEGIN TABLE CONTENTS -->
    <tr>
      <th class="tableheader" colspan="2">
        zProperty
        <br/> 
        <form method="post">
        <select class="tablevalues" 
                name="prop" 
                onchange="form.submit()">
                <option value="">
                <option tal:repeat="p here/zenPropertyIds"
                        tal:content="p"
                        tal:attributes="selected python:p==prop"/>
        </select>
        </form> 
      </th>
    </tr>
    <tr>
      <th class="tableheader" align="center">Overriding Object</th>
      <th class="tableheader" align="center">Overridden Value</th>
    </tr>
    <tr>
      <td class="tablevalues">
        <div tal:define=" link python:here.getPrettyLink()"
                tal:content="structure link"/>
      </td>
      <td class="tablevalues" tal:content="python:getattr(here, prop, '')"/>
    </tr>
    <tal:block tal:condition="not:orObjects">
            <tr>
            <td class="tablevalues" colspan="3">No Overriding Objects</td>
            </tr>
    </tal:block>
    <tal:block tal:condition="orObjects" tal:repeat="orObject orObjects"> 
    <tr tal:define="odd repeat/orObject/odd"
        tal:attributes="class python:test(odd, 'odd', 'even')">
      <td class="tablevalues">
              <div tal:define=" link python:orObject.getPrettyLink()"
                tal:content="structure link"/>
      </td>
      <td class="tablevalues" tal:content="python:getattr(orObject, prop)"/>
    </tr>
    </tal:block>
<!-- END TABLE CONTENTS -->

</tal:block>
</tal:block>
</tal:block>
</tal:block>
</tal:block>