<h2>Export Templates</h2>
<br/>
<select class="tableheader" name="dest">
    <option value="filesystem" selected="True">File System</option>
    <!-- option value="zenossdotnet">Your Zenoss.net Account</option /-->
</select>
<br/>
<div id="dialog_buttons">
<input type="submit" name="manage_exportRRDTemplates:method"
    value="OK"
    tal:attributes="onclick string:return $$('dialog').submit_form(
                        '${here/absolute_url}')" />
<input tal:attributes="id string:dialog_cancel;
                        type string:button;
                        value string:Cancel;
                        onclick string:$$('dialog').hide()" />
</div>