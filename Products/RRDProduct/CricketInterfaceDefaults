#
# Interface TargetTypes
#
targetType  StandardInterface
	ds	    =	"ifInOctets, ifOutOctets, ifInErrors,
				ifOutErrors, ifInUcastPackets, ifOutUcastPackets"
	view	=	"Octets: ifInOctets ifOutOctets,
				UcastPackets: ifInUcastPackets ifOutUcastPackets,
				Errors: ifInErrors ifOutErrors"

targetType  SubInterface
	ds	    =	"ifInOctets, ifOutOctets,
			    ifInUcastPackets, ifOutUcastPackets"
    view	=	"Octets: ifInOctets ifOutOctets,
                UcastPackets: ifInUcastPackets ifOutUcastPackets"

targetType  OctetsOnlySubInterface
	ds	    =	"ifInOctets, ifOutOctets"
    view	=	"Octets: ifInOctets ifOutOctets"

targetType  FastInterface
    ds      =   "ifHCInOctets, ifHCOutOctets, ifInErrors,
                ifOutErrors, ifHCInUcastPackets, ifHCOutUcastPackets"
    view    =   "Octets: ifHCInOctets ifHCOutOctets,
                UcastPackets: ifHCInUcastPackets ifHCOutUcastPackets,
                Errors: ifInErrors ifOutErrors"

targetType SRPInterface
    ds = "srpRingIntUcastLowPriOctetsIn, srpRingIntMcastLowPriOctetsIn,
          srpRingIntUcastHighPriOctetsIn, srpRingIntMcastHighPriOctetsIn,
          srpRingIntUcastLowPriOctetsOut, srpRingIntMcastLowPriOctetsOut,
          srpRingIntUcastHighPriOctetsOut, srpRingIntMcastHighPriOctetsOut"

targetType  DownstreamInterface
	ds	    =	"ifOutOctets, 
				ifOutErrors, ifOutUcastPackets"
	view	=	"Octets: ifOutOctets,
				UcastPackets: ifOutUcastPackets,
				Errors:  ifOutErrors"

targetType  UpstreamInterface
	ds	    =	"ifInOctets, 
				ifInErrors, ifInUcastPackets"
	view	=	"Octets: ifInOctets,
				UcastPackets: ifInUcastPackets,
				Errors:  ifInErrors"

#
# Interface Graphs
#
graph   ifInUcastPackets
    draw-as     =   AREA
    y-axis      =   "packets per second"
    units       =   "pkt/sec"
    legend      =   "Average num Packets In"

graph   ifOutUcastPackets
    y-axis      =   "packets per second"
    units       =   "pkt/sec"
    legend      =   "Average num Packets Out"

