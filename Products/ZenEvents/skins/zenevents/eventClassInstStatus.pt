<table class="zentable"  cellpadding="3" cellspacing="1">
    <tr>
      <th class="tabletitle" align="left" colspan="4">Status</th>
    </tr>
    <tr>
      <td class="tableheader">Events</td>
      <td class="tablevalues">
        <table metal:use-macro="here/deviceMacros/macros/eventrainbow" />
      </td>
      <th class="tableheader" align="left">Total Event Count</th>
      <td class="tablevalues" align="center" tal:content="here/getStatus">
        100</td>
    </tr>
</table>
<br>
<table class="zentable" cellpadding="3" cellspacing="1">
    <tr>
      <th class="tabletitle" align="left" colspan="2">EventClassInst</th>
    </tr>
    <tr>
      <td class="tableheader">Event Class Key</td>
      <td class="tablevalues" tal:content="here/eventClassKey">name</td>
    </tr>
    <tr>
      <td class="tableheader" colspan="2">Regex</td>
    </tr>
    <tr>
      <td class="tablevalues" colspan="2" tal:content="here/regex"
        tal:attributes="style here/testRegexStyle">
      regex</td>
    </tr>
    <tr>
      <td class="tableheader" colspan="2">Example</td>
    </tr>
    <tr>
      <td class="tablevalues" colspan="2" tal:content="here/example">examp</td>
    </tr>
    <tr>
      <td class="tableheader" colspan="2">Explanation</td>
    </tr>
    <tr>
      <td class="tablevalues" colspan="2" tal:content="here/explanation">
        regex</td>
    </tr>
    <tr>
      <td class="tableheader" colspan="2">Resolution</td>
    </tr>
    <tr>
      <td class="tablevalues" colspan="2" tal:content="here/resolution">
        regex</td>
    </tr>
